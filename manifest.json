{
    "name": "IPL - Swiggy 6",
    "author": "Vinit Shahdeo",
    "version": "1.1",
    "description": "A chrome extension to notify the sixers in live IPL matches. Get 60% after every sixer within 6 minutes.",

    "icons": { "128": "test.png" },
      

    "browser_action": {
      "default_title": "IPL - Swiggy 6",
      "default_icon": "test.png",
      "default_popup": "index.html"
    },
    "background": {
        "persistent":true,
        "scripts": ["js/background.js"]
      },
    "permissions": [
       "webRequestBlocking",
       "webRequest",
       "activeTab",
       "tabs",
       "storage",
       "http://*/*",
       "notifications",
       "https://*/*"
    ],
    "web_accessible_resources": [
        "ipl.mp3"
    ],
    "manifest_version": 2
  }
  